From 88d321d1512bbe4589985150997e559a3d8ac83f Mon Sep 17 00:00:00 2001
From: jjm2473 <1129525450@qq.com>
Date: Mon, 5 Jun 2023 12:40:10 +0800
Subject: [PATCH] disable pcie and usb3.0 on rk3568

---
 arch/arm/dts/rk3568-u-boot.dtsi | 15 +++++----------
 1 file changed, 5 insertions(+), 10 deletions(-)

diff --git a/arch/arm/dts/rk3568-u-boot.dtsi b/arch/arm/dts/rk3568-u-boot.dtsi
index a0678e3..99cf95d 100644
--- a/arch/arm/dts/rk3568-u-boot.dtsi
+++ b/arch/arm/dts/rk3568-u-boot.dtsi
@@ -67,6 +67,11 @@
 
 &usbdrd_dwc3 {
 	u-boot,dm-pre-reloc;
+	phys = <&u2phy0_otg>;
+	phy-names = "usb2-phy";
+	extcon = <&usb2phy0>;
+	maximum-speed = "high-speed"; /* only USB2.0 */
+	snps,dis_u2_susphy_quirk; /* only USB2.0 */
 	status = "okay";
 };
 
@@ -376,16 +381,6 @@
 	u-boot,dm-pre-reloc;
 };
 
-&pcie30phy {
-	u-boot,dm-pre-reloc;
-	status = "okay";
-};
-
-&pcie3x2 {
-	u-boot,dm-pre-reloc;
-	status = "okay";
-};
-
 &pinctrl {
 	u-boot,dm-pre-reloc;
 	status = "okay";
-- 
