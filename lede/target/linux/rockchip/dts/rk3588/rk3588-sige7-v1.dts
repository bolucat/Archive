&soc_thermal {
	trips {
		cpu_warm: cpu_warm {
			temperature = <65000>;
			hysteresis = <5000>;
			type = "active";
		};

		cpu_hot: cpu_hot {
			temperature = <85000>;
			hysteresis = <5000>;
			type = "active";
		};

		cpu_hall: cpu_hall {
			temperature = <95000>;
			hysteresis = <5000>;
			type = "active";
		};

		cpu_idle: cpu_idle {
			temperature = <20000>;
			hysteresis = <3000>;
			type = "passive";
		};
	};

	cooling-maps {
		map4 {
			trip = <&cpu_idle>;
			cooling-device = <&fan THERMAL_NO_LIMIT 2>;
		};

		map5 {
			trip = <&cpu_warm>;
			cooling-device = <&fan 2 4>;
		};

		map6 {
			trip = <&cpu_hot>;
			cooling-device = <&fan 4 6>;
		};

		map7 {
			trip = <&cpu_hall>;
			cooling-device = <&fan 6 THERMAL_NO_LIMIT>;
		};

	};
};
