+++ b/arch/arm64/boot/dts/qcom/ipq5018.dtsi
@@ -688,6 +688,197 @@
 			};
 		};
 
+		wifi0: wifi@c000000 {
+			compatible = "qcom,ipq5018-wifi";
+			reg = <0xc000000 0x1000000>;
+
